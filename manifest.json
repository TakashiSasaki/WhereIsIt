{
    "name": "WhereIsIt",
    "description": "Helps to find your belongings where it is.",
    "manifest_version": 2,
    "version": "0.0.3",
    "app": {
        "launch": {
            "local_path": "main.html"
        }
    },
    "icons": {
        "16": "icon-16.png",
        "128": "icon-128.png"
    },
    "options_page": "options.html",
    "permissions": [
        "http://localhost:80/",
        "http://localhost:5984/",
        "http://example.com/*",
        "geolocation",
        "notifications",
        "unlimitedStorage"
    ]
}
